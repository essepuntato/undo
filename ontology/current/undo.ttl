:Address a owl:Class ;
    rdfs:label "address"@en ;
    rdfs:comment "A document exemplifying keynote speeches and opening addresses and messages."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf :Communication .

:AdministrativeCircular a owl:Class ;
    rdfs:label "administrative circular"@en ;
    rdfs:comment "A document containing general information of administrative nature as well as clarifications of established rules, policies and procedures."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf :AdministrativeIssuance .

:AdministrativeInstruction a owl:Class ;
    rdfs:label "administrative instruction"@en ;
    rdfs:comment "A document that prescribes instructions and procedures for the implementation of rules and regulations, as well as formally communicate decisions on, for example, appointment, promotion, transfer and change of duty station."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf :AdministrativeIssuance .

:AdvisoryOpinion a owl:Class ;
    rdfs:label "advisory opinion"@en ;
    rdfs:comment "A document introducing an opinion rendered by a court or adjudicating body upon request."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf :CourtDecision .

:AgendaListing a owl:Class ;
    rdfs:label "agenda listing"@en ;
    rdfs:comment "A document listing agenda items to be considered at a specific meeting/s or session/s. It may report time, location, etc."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf :MeetingAgenda .

:Amendment a owl:Class ;
    rdfs:label "amendment"@en ;
    rdfs:comment "A document that provides modifications to other normative documents. The modifications could appear in different structured texts like normative and deliberative documents, debate records, etc."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf :ModificatoryInstruction .

:AmendmentList a owl:Class ;
    rdfs:label "amendment list"@en ;
    rdfs:comment "A document listing proposals for modifications submitted to official deliberative bodies for discussion. It provides modifications to other normative documents."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf :ModificatoryInstruction .

:AnnotatedAgenda a owl:Class ;
    rdfs:label "annotated agenda"@en ;
    rdfs:comment "An agenda with a brief description of agenda items, citation of related documents as well as information and other announcements."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf :MeetingAgenda .

:Annotation a owl:Class ;
    rdfs:label "annotation"@en ;
    rdfs:comment "A mechanism that allows someone to link a specific entity (e.g. a relation) to a target document (or its parts), with the intent of specifying from where that entity has been actually derived from."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/akn/ontology/allot/Reference> ;
            owl:onProperty oa:hasBody ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:unionOf ( <https://w3id.org/akn/ontology/allot/FRBRExpression> <https://w3id.org/akn/ontology/allot/FRBRItem> <https://w3id.org/akn/ontology/allot/FRBRManifestation> <https://w3id.org/akn/ontology/allot/FRBRWork> ) ] ;
            owl:onProperty oa:hasTarget ],
        oa:Annotation,
        <https://w3id.org/akn/ontology/allot/Concept> .

:Bulletin a owl:Class ;
    rdfs:label "bulletin"@en ;
