    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf :NormativeDocument .

:ValueInTimeAndContext a owl:Class ;
    rdfs:label "value in time and context"@en ;
    dc:description """This class is based on a particular ontology pattern called Time-indexed Value in Context (http://www.essepuntato.it/2012/04/tvc). It has been used by several ontologies for describing values that can be assigned/held by an entity at different times and according to different contexts. Notable ontologies that are using it are the Publishing Roles Ontology (PRO, http://purl.org/spar/pro) and the Publishing Status Ontology (PSO, http://purl.org/spar/pso).

More information are available at the following paper:

Peroni, S., Shotton, D., Vitali, F. (2012). Scholarly publishing and the Linked Data: describing roles, statuses, temporal and contextual extents. In Sack, H., Pellegrini, T. (Eds.), Proceedings of the 8th International Conference on Semantic Systems (i-Semantics 2012): 9-16. New York, New York, USA: ACM. DOI: http://dx.doi.org/10.1145/2362499.2362502 (Open Access version available at http://speroni.web.cs.unibo.it/publications/peroni-2012-scholarly-publishing-linked.pdf)"""@en ;
    rdfs:comment "The situation that involves an entity (with the object property tvc:hasValue) having a particular value (tvc:withValue) associated during a certain time interval (tvc:atTime) and within a specific context (tvc:withinContext)."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom :Interval ;
            owl:onProperty tvc:atTime ],
        [ a owl:Restriction ;
            owl:allValuesFrom :Value ;
            owl:onProperty tvc:withValue ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/akn/ontology/allot/Reference> ;
            owl:onProperty tvc:withinContext ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://w3id.org/akn/ontology/allot/Reference> ;
            owl:onProperty [ owl:inverseOf tvc:hasValue ] ],
        tvc:ValueInTime,
        <https://w3id.org/akn/ontology/allot/Concept> .

:WorkflowExecution a owl:Class ;
    rdfs:label "workflow execution"@en ;
    rdfs:comment "A collection of actions that address all the step defined in a workflow."@en ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom :Workflow ;
            owl:onProperty pwo:executes ],
        [ a owl:Restriction ;
            owl:allValuesFrom :Action ;
            owl:onProperty pwo:involvesAction ],
        pwo:WorkflowExecution,
        <https://w3id.org/akn/ontology/allot/Concept> .

:hasLifecycleRevision a owl:ObjectProperty ;
    rdfs:label "has lifecycle revision"@en ;
    rdfs:comment "It links a document version with another altered or updated version of it as consequence of an event of a lifecycle."@en ;
    rdfs:domain :DocumentVersion ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:range [ a owl:Class ;
            owl:intersectionOf ( :DocumentVersion [ a owl:Restriction ;
                        owl:onProperty [ owl:inverseOf pwo:produces ] ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( <https://w3id.org/akn/ontology/allot/Event> [ a owl:Restriction ;
                                            owl:onProperty [ owl:inverseOf :includesEvent ] ;
                                            owl:someValuesFrom :Lifecycle ] ) ] ] ) ] ;
    rdfs:subPropertyOf frbr:revision,
        <https://w3id.org/akn/ontology/allot/hasRelatedReference> .

:hasRelatedConcept a owl:ObjectProperty ;
    rdfs:label "has related concept"@en ;
    rdfs:comment "A concept that is related somehow to a term (e.g. because it represents the meaning of a particular term)."@en ;
    rdfs:domain <https://w3id.org/akn/ontology/allot/Term> ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:range <https://w3id.org/akn/ontology/allot/Concept> ;
    rdfs:subPropertyOf <https://w3id.org/akn/ontology/allot/hasRelatedReference> .

:hasStepType a owl:ObjectProperty ;
    rdfs:domain :Step ;
    rdfs:isDefinedBy <https://w3id.org/un/ontology/undo> ;
    rdfs:range :StepType ;
    rdfs:subPropertyOf <https://w3id.org/akn/ontology/allot/hasRelatedReference> .

